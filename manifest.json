{
  "manifest_version": 2,

  "name": "Hotab - find your way through the tab forst",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://*/*",
   "http://*/*",
   "tabs",
   "https://ajax.googleapis.com/"
  ]
}
